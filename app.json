{
  "expo": {
    "name": "Grandparent Coach",
    "slug": "grandparent-coach",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.grandparentcoach.app"
    },
    "android": {
      "package": "com.grandparentcoach.app",
      "permissions": ["com.android.vending.BILLING"]
    },
    "web": {},
    "scheme": "grandparentcoach",
    "plugins": [
      "expo-router",
      "expo-dev-client"
    ],
    "extra": {
      "router": {
        "origin": false
      }
    },
    "owner": "kgrant211"
  }
}
